# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=30000

# Authentication
REACT_APP_AUTH_TOKEN_KEY=pcx_auth_token
REACT_APP_USER_INFO_KEY=pcx_user_info

# Cache Configuration
REACT_APP_CACHE_DURATION_SHORT=300000
REACT_APP_CACHE_DURATION_MEDIUM=1800000
REACT_APP_CACHE_DURATION_LONG=3600000

# Analytics (if used)
REACT_APP_ANALYTICS_ID=your_analytics_id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false

# Development Settings
REACT_APP_ENABLE_DEBUG_LOGGING=false
REACT_APP_ENABLE_MOCK_API=false 